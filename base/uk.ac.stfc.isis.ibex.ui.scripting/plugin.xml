<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.python.pydev.pydev_interpreter_new_custom_entries">
      <interpreter_new_custom_entries_participant
            class="uk.ac.stfc.isis.ibex.ui.scripting.PyDevAdditionalInterpreterSettings">
      </interpreter_new_custom_entries_participant>
   </extension>
   <extension
         point="org.python.pydev.pydev_interpreter_provider">
      <interpreter_provider_participant
            class="uk.ac.stfc.isis.ibex.ui.scripting.PythonInterpreterProviderFactory">
      </interpreter_provider_participant>
   </extension>
   <extension
         point="org.python.pydev.pydev_interpreter_info_builder">
      <interpreter_info_builder_participant
            class="uk.ac.stfc.isis.ibex.ui.scripting.InterpreterInfoBuilderParticipant">
      </interpreter_info_builder_participant>
   </extension>
   <extension point="uk.ac.stfc.isis.ibex.instrument.info">
      <InstrumentInfoReceiver
            class="uk.ac.stfc.isis.ibex.ui.scripting.ConsoleSettings">
      </InstrumentInfoReceiver>
   </extension>
   <extension point="org.eclipse.ui.bindings">
      <key
            commandId="uk.ac.stfc.isis.ibex.ui.perspectives.commands.SwitchPerspective"
            schemeId="IBEX_key_scheme"
            sequence="Alt+Shift+S">
         <parameter
               id="uk.ac.stfc.isis.ibex.ui.perspectives.commands.perspectiveID"
               value="uk.ac.stfc.isis.ibex.ui.scripting.perspective">
         </parameter>
      </key>
   </extension>
</plugin>
