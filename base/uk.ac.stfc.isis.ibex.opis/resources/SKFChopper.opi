        <scriptText><![CDATA[from org.csstudio.opibuilder.scriptUtil import PVUtil
from org.csstudio.opibuilder.scriptUtil import ConsoleUtil

container = display.getWidget("Tabs")

# enable tabs

for i in range(0,5):

	if pvArray[i].isConnected():
		container.setPropertyValue("tab_%s_enabled" % i, "true")
	else:
		container.setPropertyValue("tab_%s_enabled" % i, "false")
]]></scriptText>
