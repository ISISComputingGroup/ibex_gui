          <scriptName>Baffle motion</scriptName>
          <scriptText><![CDATA[from org.csstudio.opibuilder.scriptUtil import PVUtil

print("Hello World")

baffle_home = display.getWidget("Baffle home").getPropertyValue("x") + display.getWidget("Baffle home").getPropertyValue("width")*0.5


pos_x =  -100*PVUtil.getDouble(pvs[0]) + baffle_home
print("baffle pos: " + pos_x)
widget.setPropertyValue("x", pos_x)

]]></scriptText>
