    <rules>
      <rule name="IF motor moving THEN change background colour" prop_id="background_color" out_exp="false">
        <exp bool_exp="pv0==0">
          <value>
            <color red="213" green="255" blue="213" />
          </value>
        </exp>
        <pv trig="true">$(PV_ROOT)$(MTR).DMOV</pv>
      </rule>
      <rule name="IF at limit switch THEN change border colour" prop_id="border_color" out_exp="false">
        <exp bool_exp="pv0==1||pv1==1">
          <value>
            <color name="ISIS_Shutter_Moving" red="255" green="150" blue="0" />
          </value>
        </exp>
        <pv trig="true">$(HIGH_LIMIT)</pv>
        <pv trig="true">$(LOW_LIMIT)</pv>
      </rule>
      <rule name="IF at limit switch THEN change border thickness" prop_id="border_width" out_exp="false">
        <exp bool_exp="pv0==1||pv1==1">
          <value>2</value>
        </exp>
        <pv trig="true">$(LOW_LIMIT)</pv>
        <pv trig="true">$(HIGH_LIMIT)</pv>
      </rule>
    </rules>
