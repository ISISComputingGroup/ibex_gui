              <scriptText><![CDATA[from org.csstudio.opibuilder.scriptUtil import PVUtil
from org.csstudio.opibuilder.scriptUtil import ConsoleUtil

targetPV = display.getWidget('SetPointTextChanger').getPV()
sourceBtn = display.getWidget('sourceBtnChanger') 

menu = display.getWidget('SetPosnBtnChanger')
value = pvs[0].getValue()    
    
if value is None:
    values = "None_Found END"
else:
    values = "".join(chr(value.getData().getByte(i)) for i in range(value.getData().size()))

actionList = menu.getPropertyValue('actions').getActionsList()
actionClass = sourceBtn.getPropertyValue('actions').getActionsList()[0].__class__
actionList.clear()

for name in values.split():
	if name=="END":
		break
	if len(name)>0:
		action = actionClass()
		action.setPropertyValue('pv_name', targetPV.getName())
		action.setPropertyValue('description', name)
		action.setPropertyValue('value', name)
		actionList.add(action)

]]></scriptText>
