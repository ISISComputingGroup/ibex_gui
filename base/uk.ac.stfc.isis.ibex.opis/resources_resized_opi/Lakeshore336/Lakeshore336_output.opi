        <path pathString="EmbeddedJs" checkConnect="true" sfe="false" seoe="false">
          <scriptName>Dynamic Macros</scriptName>
          <scriptText>importPackage(Packages.org.csstudio.opibuilder.scriptUtil);

var value = PVUtil.getString(pvs[0]);
var lastChar = value.substring(value.length() - 1);

var inputViewOpi = "Lakeshore336_input_CSS_defaults.opi";
var noInputViewOpi = "Lakeshore336_input_none.opi";
var currentOpi =String(widgetController.getPropertyValue("opi_file"));

if (lastChar == "A" || lastChar == "B" || lastChar == "C" || lastChar == "D") {
	// Create a new Macro Input
	var macroInput = DataUtil.createMacrosInput(true);
	
	// Put a macro in the new Macro Input	
	macroInput.put("INPUT", lastChar);

	// Set the Macro Input of the linking container to this new Macro Input
	widgetController.setPropertyValue("macros", macroInput);

	// Reload the OPI in the linking container
	// by setting the value property with forcing fire option as true
	var opiToLoad = currentOpi.replace(noInputViewOpi, inputViewOpi);
	widgetController.setPropertyValue("opi_file", opiToLoad, true);
	//widgetController.setPropertyValue("opi_file", widgetController.getPropertyValue("opi_file"), true);
} 
else if (value == "None") {
	// Set the OPI for No Control Input
	var opiToLoad = currentOpi.replace(inputViewOpi, noInputViewOpi);
	widgetController.setPropertyValue("opi_file", opiToLoad, true);
}</scriptText>
          <pv trig="true">$(PV_ROOT):CTRL_IN$(OUT)</pv>
        </path>
      </scripts>
